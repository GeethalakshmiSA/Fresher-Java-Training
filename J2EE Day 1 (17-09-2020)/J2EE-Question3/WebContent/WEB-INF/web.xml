<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>SourceServlet</servlet-name>
    <servlet-class>com.cts.SourceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SourceServlet</servlet-name>
    <url-pattern>/SourceServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DestinationServlet</servlet-name>
    <servlet-class>com.cts.DestinationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DestinationServlet</servlet-name>
    <url-pattern>/DestinationServlet</url-pattern>
  </servlet-mapping>
</web-app>